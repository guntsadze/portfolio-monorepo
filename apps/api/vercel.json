{
  "version": 3,  
  "builds": [], 
  "routes": [  
    {
      "src": "/api/(.*)",
      "dest": "/api/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "/"  
    }
  ],
  "env": { 
    "NEXT_PUBLIC_API_BASE_URL": "@api_base_url"
  },
  "functions": { 
    "api/**/*.js": {
      "maxDuration": 10
    }
  }
}